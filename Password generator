import random
b = 'abcdefghijklmnopqrstuvwxyz0123456789!@#$%^&*'
try :
    c = int ( input ("Type the lenght of the password"))
    d = int(input ("Enter the number of password you want :"))
    str =''
    for i in range (0,d):
        for j in range (0,c):
            str = str + random.choice(b)
        print (f"password {i+1} is {str}") 
        str = ''
except ValueError :
    print ("invalid . it must be in integer ")
